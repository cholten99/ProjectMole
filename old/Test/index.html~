    <script>
      // onLoad function
      $(function() {
        initialiseTimeTrigger("renderTrigger");

        // Update the time
        var unixtime = new Date().getTime();
        var date = new Date();
        var now = date.toUTCString();
        $("#theTime").html("Oh and the time the page was loaded : " + now);


      });

      function triggerUpdated() {
        location.reload(true);
      }

    </script>
  
